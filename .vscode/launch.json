{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug dartt_dashboard",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/dartt_dashboard",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build",
      "environment": [],
      "externalConsole": false,
      "linux": {
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      },
      "windows": {
        "type": "cppvsdbg",
        "program": "${workspaceFolder}/build/Debug/dartt_dashboard.exe",
        "cwd": "${workspaceFolder}/build/Debug"
      },
      "preLaunchTask": "CMake: build"
    }
  ]
}
